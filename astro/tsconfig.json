{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "src/node_modules",
    "dist/"
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "verbatimModuleSyntax": false,
  }
}