---
interface Props {
    pageTitle: string;
    cardPath: string;
}
const { cardPath, pageTitle } = Astro.props;
import Header from "@/components/Header.astro";
import Fotter from "@/components/Fotter.astro";
import Blanklayout from "@/layouts/Blanklayout.astro";
---

<Blanklayout cardPath={cardPath} pageTitle={pageTitle}>
    <Header slot="header" />
    <slot />
    <Fotter slot="footer" />
</Blanklayout>
